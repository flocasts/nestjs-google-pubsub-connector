{
  "name": "@flosportsinc/nestjs-google-pubsub-microservice",
  "version": "0.0.1",
  "description": "A NestJS Custom Transport Strategy for Google PubSub",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --max-workers=1",
    "build": "tsc",
    "prepare": "husky install",
    "format": "prettier \"lib/**/*.ts\" \"examples/**/*.ts\"  -c",
    "format:fix": "npm run format -- --write"
  },
  "keywords": [
    "nestjs",
    "google",
    "pubsub"
  ],
  "author": "Flosports",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/microservices": "^7.6.15",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.10.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">=7.0 && <8",
    "@nestjs/core": ">=7.0 && <8",
    "@nestjs/microservices": ">=7.0 && <8",
    "reflect-metadata": "^0.1.13",
    "rxjs": ">=6 && <7"
  },
  "lint-staged": {
    "{lib,test,examples}/**/*.ts": "prettier --write",
    "{lib,apps,examples,libs,test}/**/*.ts": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run build && npm run test"
    }
  }
}
